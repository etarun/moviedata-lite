<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.doordash.doordashlite">

    <!--Internet Permission-->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.movies.moviedata.MovieDataApplication"
        android:allowBackup="true"
        android:icon="@drawable/images"
        android:label="@string/app_name"
        android:roundIcon="@drawable/images"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar" >

        <activity android:name="com.movies.moviedata.mainActivityComponent.MainActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.movies.moviedata.ui.movieDetails.MovieDetailActivity" />
    </application>
</manifest>
